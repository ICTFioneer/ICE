"routing": {
  "config": {
    "routerClass": "sap.m.routing.Router",
    "viewType": "XML",
    "async": true,
    "viewPath": "ice.view",
    "controlAggregation": "pages",
    "controlId": "app",
    "clearControlAggregation": false
  },
  "routes": [
    {
      "name": "RouteMain",
      "pattern": ":?query:",
      "target": ["TargetMain"]
    },
   {
          "name": "RouteDetail",
          "pattern": "detail/{CompanyCode}/{TradingPartner}/{FinalBreakCode}:?query:",
          "target": "TargetDetail"
        },

        {
          "name": "RouteDetailL2Product",
          "pattern": "detailL2/product/{CompanyCode}/{TradingPartner}/{FinalBreakCode}/{KeyValue}:?query:",
          "target": "TargetDetailL2Product"
        },
        {
          "name": "RouteDetailL2Segment",
          "pattern": "detailL2/segment/{CompanyCode}/{TradingPartner}/{FinalBreakCode}/{KeyValue}:?query:",
          "target": "TargetDetailL2Segment"
        },
        {
          "name": "RouteDetailL2Category",
          "pattern": "detailL2/category/{CompanyCode}/{TradingPartner}/{FinalBreakCode}/{KeyValue}:?query:",
          "target": "TargetDetailL2Category"
        }


  ],
  "targets": {
    "TargetMain": {
      "viewType": "XML",
      "transition": "slide",
      "clearControlAggregation": false,
      "viewId": "Main",
      "viewName": "Main"
    },
     "TargetDetail": {
          "viewName": "Detail",
          "viewId": "Detail",
          "viewLevel": 2
        },

        "TargetDetailL2Product": {
          "viewName": "DetailL2",
          "viewId": "DetailL2Product",
          "viewLevel": 3
        },
        "TargetDetailL2Segment": {
          "viewName": "DetailL2",
          "viewId": "DetailL2Segment",
          "viewLevel": 3
        },
        "TargetDetailL2Category": {
          "viewName": "DetailL2",
          "viewId": "DetailL2Category",
          "viewLevel": 3
        }
  }
}
